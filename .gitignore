# Ignore everything in the 'emacs' directory
/emacs/*

# But don't ignore these files and directories
!/emacs/init.el
!/emacs/early-init.el
!/emacs/lisp/
!/emacs/setup/
!/emacs/scripts/

# Ignore everything in the 'zsh' directory
/zsh/*

# But don't ignore these files and directories
!/zsh/zshrc
!/zsh/zshenv
!/zsh/zprofile
!/zsh/lscolors.sh
!/zsh/scripts/
!/zsh/functions/
